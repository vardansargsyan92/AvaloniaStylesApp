<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:themes="clr-namespace:AvaloniaStylesApp.Themes">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="BackgroundBrush">Coral</SolidColorBrush>
            <SolidColorBrush x:Key="ForegroundBrush">White</SolidColorBrush>
        </ResourceDictionary>

        <ResourceDictionary x:Key="{x:Static themes:ThemeVariants.Custom}">
            <SolidColorBrush x:Key="BackgroundBrush">Purple</SolidColorBrush>
            <SolidColorBrush x:Key="ForegroundBrush">Yellow</SolidColorBrush>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
